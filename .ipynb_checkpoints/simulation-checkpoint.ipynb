    "def get_situation(w, b):\n",
    "    if 0 <= w and w <= 3:\n",
    "        if 1 <= b and b <= 36:\n",
    "            return 1\n",
    "        elif 36 <= b and b <= 96:\n",
    "            return 2\n",
    "        else:\n",
    "            return 3\n",
    "    elif 4 <= w and w <= 6:\n",
    "        if 1 <= b and b <= 36:\n",
    "            return 4\n",
    "        elif 36 <= b and b <= 96:\n",
    "            return 5\n",
    "        else:\n",
    "            return 6\n",
    "    else:\n",
    "        if 1 <= b and b <= 36:\n",
    "            return 7\n",
    "        elif 36 <= b and b <= 96:\n",
    "            return 8\n",
    "        else:\n",
    "            return 9"
   ]
