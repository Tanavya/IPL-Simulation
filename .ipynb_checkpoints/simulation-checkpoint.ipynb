    "def get_outcome(r, wicket):\n",
    "    if wicket is True:\n",
    "        return 1\n",
    "    elif r == 0:\n",
    "        return 2\n",
    "    elif r == 1:\n",
    "        return 3\n",
    "    elif r == 2:\n",
    "        return 4\n",
    "    elif r == 3:\n",
    "        return 5\n",
    "    elif r == 4:\n",
    "        return 6\n",
    "    elif r == 6:\n",
    "        return 7\n",
    "    else:\n",
    "        return 0"
   ]
